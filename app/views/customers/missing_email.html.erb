
<section class="section">
  <div class="container">
    <h1 class="title">Customers Missing Email Data</h1>
    <span class="tag is-link is-light is-medium mb-4"><%= link_to "Back to Customers", customers_path %></span>
    <div class="columns is-multiline">
      <% @customers.each do |customer| %>
        <div class="column is-one-quarter">
          <%= render partial: "customer", locals: { customer: customer } %>
        </div>
      <% end %>
    </div>
  </div>
</section>
